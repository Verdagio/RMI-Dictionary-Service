<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" id="WebApp_ID"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<display-name>Dictionary lookup</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>DictionaryServlet</servlet-name>
		<servlet-class>ie.gmit.sw.DictionaryServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DictionaryServlet</servlet-name>
		<url-pattern>/ServletPath</url-pattern>
	</servlet-mapping>
</web-app>